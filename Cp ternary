#include <stdio.h> 

int main()

{

int a, b, max;

// Input two numbers

printf("Enter the first number (a):"); 

scanf("%d",&a);

printf("Enter the second number (b):"); 

scanf("%d",&b);

// Use conditional/ternary operator to find the maximum 

max = (a>b)?a:b;

// Output the result

printf("The larger number is: %d\n", max); 

return 0;

}
