#include<stdio.h>
#include<conio.h>
void main()
{
int n,n1,rem,num=0;
clrscr();
printf("enter a positive integer:");
scanf("%d",&n);
n1=n;
while(n1!=0)
{
rem=n1%10;
num+=rem*rem*rem;
n1/=10;
}
if(num==n)
printf("\n%d is an armstrong number.",n);
else
printf("\n %d is not armstrong number.",n);
getch();
}
