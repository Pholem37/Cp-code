#include<stdio.h>
#include<conio.h>

void main()
{
int num,i;
clrscr();

//Asking user to input a number
printf("Enter a number to generate its multiplication table:");
scanf("%d",&num);

//Generating table using a for loop
printf("Multiplication table of %d:\n",num);
for(i=1;i<=12;i++)
{
printf("%d x %d=%d\n",num,i,num*i);
}

getch();
}
