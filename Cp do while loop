#include<stdio.h>
#include<conio.h>
void main()
{
int n,n1,rem,num=0;
clrscr();
printf("Enter a positive integer:");
scanf("%d",&n);
n1=n;
do
{
rem=n1%10;
num+=rem*rem*rem;
n1/=10;
}while(n1!=0)
if(num==n)
printf("\n%d is an Armstrong number.",n);
else
printf("\n%d is not an Armstrong number.",n);
getch();
}
